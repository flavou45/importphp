{
    "name": "youlead-bow/importphp",
    "description": "Data import/export workflow ",
    "keywords": [ "data", "import", "export", "doctrine" ],
    "license": "MIT",
    "homepage": "https://github.com/youlead-bow/importphp",
    "require": {
        "php": ">=8.3",
        "psr/log": "~2.0",
        "seld/signal-handler": "^1.0",
        "symfony/property-access": "^7.0",
        "symfony/validator": "^7.0",
        "doctrine/common": "^3.2",
        "doctrine/dbal": "^3.0|4.0"
    },
    "autoload": {
        "psr-4": {
            "Import\\": "src/"
        }
    },
    "require-dev": {
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "doctrine/instantiator": "^1.4",
        "doctrine/orm": "^2.0|^3.0",
        "phpstan/phpstan": "^1.4",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
